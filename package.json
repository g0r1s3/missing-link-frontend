{
  "name": "missing-link-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "gen:manifest": "node scripts/gen-component-manifest.cjs",
    "gen:manifest:all": "PUBLIC_ONLY=0 node scripts/gen-component-manifest.cjs",
    "gen:manifest:dom": "INCLUDE_DOM=1 node scripts/gen-component-manifest.cjs",
    "gen:manifest:md": "DOC_MD=1 node scripts/gen-component-manifest.cjs",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:js:fix": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "lint:css": "stylelint \"src/**/*.{css,scss}\"",
    "lint:css:fix": "stylelint \"src/**/*.{css,scss}\" --fix"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^61.0.2",
    "fast-glob": "^3.3.3",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "react-docgen-typescript": "^2.4.0",
    "stylelint": "^16.24.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-declaration-strict-value": "^1.10.11",
    "stylelint-order": "^7.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.2"
  }
}
